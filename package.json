{
    "name": "mp3-file-analysis",
    "version": "0.0.1",
    "description": "A service which returns a frame count for an MP3 file",
    "main": "./.dist/start.js",
    "scripts": {
        "start": "PRETTY_PRINT=false node .dist/start.js",
        "start:dev": "PRETTY_PRINT=true nodemon start.ts -r ts-node/register --inspect=9229",
        "test": "LOGGING_ENABLED=false vitest --watch=false",
        "test:watch": "LOGGING_ENABLED=false vitest",
        "test:watch:verbose": "PRETTY_PRINT=true LOG_LEVEL=debug vitest",
        "build": "tsc -p tsconfig.build.json",
        "build:watch": "tsc -p tsconfig.build.json --watch",
        "format": "prettier --write \"**/*.{ts,tsx,md,js,jsx,cjs,mjs}\"",
        "lint": "eslint --max-warnings 0",
        "lint:fix": "pnpm lint --fix"
    },
    "author": "Mark Hammond",
    "dependencies": {
        "@fastify/cors": "^11.1.0",
        "@fastify/multipart": "^9.3.0",
        "@fastify/request-context": "^6.2.1",
        "@fastify/swagger": "^9.6.1",
        "@fastify/swagger-ui": "^5.2.3",
        "@types/node": "^20.19.25",
        "convict": "^6.2.4",
        "fastify": "^5.6.2",
        "pino": "^10.1.0",
        "pino-pretty": "^13.1.3",
        "typescript": "5.2.2"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.3",
        "@eslint/js": "^9.39.1",
        "@types/convict": "^6.1.6",
        "axios": "^1.13.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-check-file": "^3.3.1",
        "eslint-plugin-import": "^2.32.0",
        "form-data": "^4.0.5",
        "globals": "^16.5.0",
        "is-port-reachable": "^3.0.0",
        "nodemon": "^3.1.0",
        "prettier": "^3.7.3",
        "ts-node": "^10.9.2",
        "typescript-eslint": "^8.48.1",
        "vitest": "^3.2.4"
    },
    "packageManager": "pnpm@9.15.9",
    "engines": {
        "node": ">=24"
    }
}