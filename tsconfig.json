{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
     "allowJs": true,
     "checkJs": false,              
     "outDir": ".dist",     
    "esModuleInterop": true ,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "test-reports/**", "**/*.test.ts"]
}
